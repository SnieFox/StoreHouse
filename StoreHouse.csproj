<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\Logo %28Меню%29.png" />
    <None Remove="Images\Logo%28Склад%29.png" />
    <None Remove="Images\LogoImage.png" />
    <None Remove="Images\Main page.png" />
